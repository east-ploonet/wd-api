<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.saltlux.aice_fe.pc.join.dao.CompanyDeptDao">

    <update id="updateNameByPk">
        UPDATE tbl_company_dept
           SET fd_dept_name = #{fd_dept_name}
         WHERE pk_company_dept = #{pk_company_dept}
    </update>

</mapper>
